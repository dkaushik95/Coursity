<p id="notice"><%= notice %></p>

<h1>Courses</h1>

<table>
  <thead>
    <tr>
      <th>C name</th>
      <th>Price</th>
      <th>Description</th>
      <th>Prof</th>
      <th>Units</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.c_name %></td>
        <td><%= course.price %></td>
        <td><%= course.description %></td>
        <td><%= course.prof %></td>
        <td><%= course.units %></td>
        <td><%= link_to 'Show', course %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

